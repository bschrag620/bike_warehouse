<h1>Shopping cart</h1>
<%= render 'cart', cart: cart_lookup, total: cart_total %>
<%= button_to 'Checkout', user_checkout_path(current_user.id), :disabled => !logged_in?, :method => :get %> <%= button_to "Continue shopping", root_path, :method => :get %> 
<% if !logged_in? %>
	You must <%= link_to "log in", login_path %> to proceed with checkout
<% end %>