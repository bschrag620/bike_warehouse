<h1>Users index</h1>
<table>
	<tr>
		<th>Username</th>
		<th>Email</th>
		<th></th>
		<th></th>
	</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= link_to "Edit", edit_admin_user_path(user) %></td>
		<td><%= link_to 'View activity', admin_user_purchases_path(user) %></td>
	</tr>
<% end %>
</table>