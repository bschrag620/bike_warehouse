<%= render 'session/cart', cart: cart_lookup, total: cart_total %>

<%= form_for(:shipping_address) do |f| %>
	<%= label_text :line_1, f %>
	<%= label_text :line_2, f %>
	<%= label_text :city, f %>
	<%= label_text :state, f %>
	<%= label_text :zip, f %>
	<%= label_tag :default_shipping, "Default shipping address?" %>
	<%= f.check_box :default_shipping %>
<% end %>